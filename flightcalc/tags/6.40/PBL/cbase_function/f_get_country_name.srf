global function string f_get_country_name (long lcountry);string sCountry

select ccountry_ger into :sCountry from sys_countries where ncountry_key = :lCountry;

if sqlca.sqlcode = 0 then
	return sCountry
else
	f_db_error(sqlca, "f_get_country_name")
	return "Error"
end if

end function

