global function string f_check_three_letter_code_id (long lcode);// ---------------------------------
// Eingegebenen 3-Letter-Code
// pr$$HEX1$$fc00$$ENDHEX$$fen
// ---------------------------------

String sCode

SELECT sys_three_letter_codes.ctlc
 INTO :sCode  
 FROM sys_three_letter_codes  
WHERE sys_three_letter_codes.ntlc_key  = :lCode   ;

if sqlca.sqlcode <> 0 then
	return "N/A"
else
	return sCode
end if


end function

