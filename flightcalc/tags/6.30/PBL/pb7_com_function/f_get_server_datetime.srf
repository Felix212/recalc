global function datetime f_get_server_datetime ();
datetime dtRet

SELECT SYSDATE INTO :dtRet FROM DUAL;

if sqlca.sqlcode = 0 then
	return dtRet
else
	f_db_error(sqlca, "f_get_server_datetime")
	return(Datetime(today(), now()))
end if






end function

