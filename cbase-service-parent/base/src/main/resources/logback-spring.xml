<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<include resource="org/springframework/boot/logging/logback/defaults.xml" />
	<include resource="logback-spring-defaults.xml" />

	<jmxConfigurator />

	<root level="INFO">
		<appender-ref ref="APPLICATION_CONSOLE" />
		<appender-ref ref="APPLICATION_FILE" />
	</root>

	<logger name="org.springframework.security" level="WARN" />
	<logger name="org.springframework.web.servlet.mvc.annotation" level="WARN" />
	<logger name="org.springframework.web.servlet.handler" level="WARN" />
	<logger name="org.springframework.security.web.csrf.CsrfFilter" level="DEBUG" />
	<logger name="org.springframework.security.web.session.SessionManagementFilter" level="DEBUG" />

